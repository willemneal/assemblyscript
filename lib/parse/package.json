{
  "name": "@assemblyscript/parse",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "asbuild": "../../bin/asc assembly/index.ts -O3 -b build/index.wasm -t build/index.wat --importMemory --debug --validate -d build/index.d.ts",
    "asbuild:untouched": "../../bin/asc assembly/index.ts -b build/index.wasm -t build/index.wat --debug --importMemory --validate -d build/index.d.ts",
    "build": "npm run asbuild && webpack --mode development --display-modules",
    "asbuild:test": "npm run asbuild:untouched && ../../bin/asc tests/assembly/index.ts -b build/test.wasm -t build/test.wat --debug --sourceMap --validate",
    "test": "ts-node tests/"
  },
  "files": [
    "package.json",
    "index.d.ts",
    "index.js",
    "index.js.map",
    "src/",
    "README.md"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/webassembly-js-api": "0.0.1",
    "assemblyscript": "github:AssemblyScript/assemblyscript",
    "ts-loader": "^5.2.1",
    "ts-node": "^6.2.0",
    "typescript": "^3.3.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "bin": {
    "ap": "bin/ap.ts"
  }
}
